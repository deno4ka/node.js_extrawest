{
  "compilerOptions": {
    "allowJs": false,
    "declaration": true,
    "module": "es6",
    "target": "es2018",
    "lib": [
      "es2018"
    ],
    "moduleResolution": "node",
    "rootDir": ".",
    "sourceMap": true,
    "alwaysStrict": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "skipLibCheck": true,
    "removeComments": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "listFiles": true,
    "pretty": true,
    "outDir": "built/local"
  }
}
